{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Framework Launch (IIS Express)",
            "type": "clr",
            "request": "launch",
            "program": "C:\\Program Files\\IIS Express\\iisexpress.exe",
            "args": [
                "/path:${workspaceFolder}\\aspnet-get-started",
                "/port:5000"
            ],
            "cwd": "${workspaceFolder}\\aspnet-get-started",
            "stopAtEntry": false,
            "sourceFileMap": {
                "/Views": "${workspaceFolder}\\aspnet-get-started\\Views"
            }
        },
        {
            "name": ".NET Framework Attach",
            "type": "clr",
            "request": "attach"
        }
    ]
}
